      "API": {
        "description": "Metainformações sobre a API",
        "type": "object",
        "required": [
          "added",
          "preferred",
          "versions"
        ],
        "properties": {
          "added": {
            "description": "Carimbo de data/hora quando a API foi adicionada pela primeira vez ao diretório",
            "type": "string",
            "format": "date-time"
          },
          "preferred": {
            "description": "Recommended version",
            "type": "string"
          },
          "versions": {
            "description": "List of supported versions of the API",
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/ApiVersion"
            },
            "minProperties": 1
          }
        },
        "additionalProperties": false
      },
      "ApiVersion": {
        "type": "object",
        "required": [
          "added",
          "updated",
          "swaggerUrl",
          "swaggerYamlUrl",
          "info"
        ],
        "additionalProperties": false
      },
      "Metrics": {
        "description": "Lista de métricas básicas",
        "type": "object",
        "required": [
          "numSpecs",
          "numAPIs",
          "numEndpoints"
        ],
        "properties": {
          "numSpecs": {
            "description": "Número de especificações de API, incluindo diferentes versões da mesma API",
            "type": "integer",
            "minimum": 1
          },
          "numAPIs": {
            "description": "Número de APIs",
            "type": "integer",
            "minimum": 1
          },
          "numEndpoints": {
            "description": "Número total de endpoints dentro de todas as especificações",
            "type": "integer",
            "minimum": 1
          }
        },
